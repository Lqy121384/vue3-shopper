{
  "name": "vue3-ts-shop",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "dev:server": "ts-node-dev --project tsconfig.server.json --respawn --transpile-only src/app.ts",
    "dev:mock": "node mock-server.js",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "seed": "ts-node-dev --project tsconfig.server.json --transpile-only src/scripts/seedProducts.ts",
    "server": "node -r dotenv/config server/index.js",
    "start": "node -r dotenv/config server/index.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@faker-js/faker": "^9.6.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.9",
    "@vueuse/core": "^10.7.2",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.5.0",
    "echarts": "^5.5.0",
    "element-plus": "^2.9.7",
    "express": "^4.21.2",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.21",
    "marked": "^15.0.8",
    "mongoose": "^8.13.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "pinia": "^2.1.7",
    "sass": "^1.71.1",
    "vue": "^3.4.29",
    "vue-echarts": "^6.6.8",
    "vue-i18n": "^9.9.1",
    "vue-router": "^4.3.3",
    "vue3-marquee": "^1.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.5",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20.17.28",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "canvas": "^3.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "mockjs": "^1.1.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "sass-loader": "^14.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.3.1",
    "vue-tsc": "^2.0.21"
  }
}
